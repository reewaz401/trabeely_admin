    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-select": "^4.1.0",
    "react-spinners": "^0.10.4",
    "react-toast-notifications": "^2.4.3",
    "reactstrap": "8.9.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "5.2.0",
