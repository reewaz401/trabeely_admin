{
  "name": "TRABEELY-admin-panel",
  "version": "1.2.0",
  "description": "Controlling packages and travel admin system",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "author": "Rishav",
  "license": "SCOUT",
  "bugs": {
    "url": "https://github.com"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "classnames": "2.2.6",
    "date-fns": "^2.17.0",
    "formik": "^2.2.6",
    "install": "^0.13.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-precise-range-plugin": "^1.3.0",
    "multiselect-react-dropdown": "^1.6.7",
    "node-sass": "4.14.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "14.6.3",
    "npm": "^7.6.0",
    "react": "^17.0.1",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-filter": "^1.3.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-confirm-alert": "^2.7.0",
    "react-date-range": "^1.1.3",
    "react-datetime": "3.0.4",
    "react-datetime-picker": "^3.1.0",
    "react-dom": "17.0.1",
    "react-floating-action-button": "^1.0.5",
